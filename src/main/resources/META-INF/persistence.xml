<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="joined-table" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>com.mycompany.joinedtables.entidades.ISO_OID</class>
    <class>com.mycompany.joinedtables.entidades.InternetID</class>
    <class>com.mycompany.joinedtables.entidades.UUID</class>
    <class>com.mycompany.joinedtables.entidades.AccessGroupRef</class>
    <class>com.mycompany.joinedtables.entidades.ArchetypeID</class>
    <class>com.mycompany.joinedtables.entidades.GenericID</class>
    <class>com.mycompany.joinedtables.entidades.HierObjectID</class>
    <class>com.mycompany.joinedtables.entidades.LocatableRef</class>
    <class>com.mycompany.joinedtables.entidades.ObjectID</class>
    <class>com.mycompany.joinedtables.entidades.ObjectRef</class>
    <class>com.mycompany.joinedtables.entidades.ObjectVersionID</class>
    <class>com.mycompany.joinedtables.entidades.PartyRef</class>
    <class>com.mycompany.joinedtables.entidades.TemplateID</class>
    <class>com.mycompany.joinedtables.entidades.TerminologyID</class>
    <class>com.mycompany.joinedtables.entidades.VersionTreeID</class>
    <class>com.mycompany.joinedtables.entidades.UID</class>
    <class>com.mycompany.joinedtables.entidades.UIDBasedID</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/identification_v3"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="h4ll"/>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
      <property name="hibernate.archive.autodetection" value="false"/>
      <property name="javax.persistence.schema-generation.database.action" value="none"/>
    </properties>
  </persistence-unit>
</persistence>
